        if keyTime is 0 # 未入力
            success = 0
        # Great
        else if great >= keyFrame >= 0 or keyFrame > bpmsec - great
            success = 1
        # Good
        else if great + good >= keyFrame >= 0 or keyFrame > bpmsec - great - good
            success = 2
        # Miss
        else 
